{
  "name": "lectoraat-smart-energy-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "lectoraat-smart-energy",
  "sourceRoot": "apps/lectoraat-smart-energy-web/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/lectoraat-smart-energy-web",
        "index": "apps/lectoraat-smart-energy-web/src/index.html",
        "browser": "apps/lectoraat-smart-energy-web/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/lectoraat-smart-energy-web/tsconfig.app.json",
        "assets": [
          "apps/lectoraat-smart-energy-web/src/favicon.ico",
          "apps/lectoraat-smart-energy-web/src/assets",
          "apps/lectoraat-smart-energy-web/src/manifest.webmanifest"
        ],
        "styles": [
          "apps/lectoraat-smart-energy-web/src/styles.css"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "serviceWorker": "apps/lectoraat-smart-energy-web/ngsw-config.json"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "lectoraat-smart-energy-web:build:production"
        },
        "development": {
          "buildTarget": "lectoraat-smart-energy-web:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "lectoraat-smart-energy-web:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/lectoraat-smart-energy-web/jest.config.ts"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "lectoraat-smart-energy-web:build",
        "staticFilePath": "dist/apps/lectoraat-smart-energy-web/browser"
      }
    }
  }
}